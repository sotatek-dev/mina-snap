{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "license": "(MIT-0 OR Apache-2.0)",
  "scripts": {
    "setup": "yarn install --frozen-lockfile && yarn allow-scripts",
    "start:all": "yarn clean && yarn install && yarn build && yarn start",
    "start": "env-cmd -f .env.production craco start",
    "build": "env-cmd -f .env.production craco build",
    "test": "craco test --coverage --passWithNoTests",
    "eject": "craco eject",
    "lint:react": "eslint ./src/**/*.ts ./src/**/*.tsx -f json -o eslint-report.json",
    "lint:misc": "prettier --check ./src/**/*.js ./src/**/*.ts ./src/**/*.tsx",
    "pretty": "prettier --write ./src/**/*.js  ./src/**/*.ts ./src/**/*.tsx",
    "lint": "yarn lint:react && yarn lint:misc",
    "clean": "rimraf build/*",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "type:check": "tsc --pretty --noEmit",
    "release": "sh ../../scripts/release.sh",
    "dev": "env-cmd -f .env.development craco build && env-cmd -f .env.development craco start"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@headlessui/react": "^1.6.4",
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/providers": "^9.0.0",
    "@mui/material": "^5.11.9",
    "@mui/styled-engine-sc": "^5.11.9",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^1.3.3",
    "bignumber.js": "^9.1.1",
    "bs58check": "^3.0.1",
    "env-cmd": "^10.1.0",
    "ethers": "^5.6.8",
    "moment": "^2.29.4",
    "o1js": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropdown": "^1.10.0",
    "react-is": "^18.2.0",
    "react-popper-tooltip": "^4.3.0",
    "react-qr-code": "^2.0.7",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.8.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.58.1",
    "semver": "^7.3.7",
    "styled-components": "5.3.3",
    "toastr2": "^3.0.0-alpha.18",
    "web-vitals": "^2.1.4",
    "web3": "^1.8.2"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@metamask/eslint-config": "^10.0.0",
    "@metamask/eslint-config-jest": "^10.0.0",
    "@metamask/eslint-config-nodejs": "^10.0.0",
    "@metamask/eslint-config-typescript": "^10.0.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/semver": "^7.3.10",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "webpack": "5"
  }
}
